{
    "name": "cargonaut",
    "version": "0.0.1",
    "description": "A platform for rides",
    "private": true,
    "license": "MIT",
    "homepage": "https://europe-west1-cargonaut.cloudfunctions.net/api",
    "repository": {
        "type": "git",
        "url": "https://github.com/clemenscodes/cargonaut"
    },
    "engines": {
        "node": "14"
    },
    "main": "main.js",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "start": "node dist/src/main",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "doc": "compodoc -p tsconfig.doc.json --language de-DE",
        "doc:coverage": "compodoc -p tsconfig.doc.json --language de-DE --coverageTest",
        "doc:serve": "compodoc -p tsconfig.doc.json --language de-DE -r 8001 -s",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "test:ci": "jest --coverage --ci --reporters=default --reporters=jest-junit",
        "deploy": "firebase deploy --only functions",
        "logs": "firebase functions:log"
    },
    "dependencies": {
        "@nestjs/axios": "0.0.3",
        "@nestjs/common": "^8.1.2",
        "@nestjs/config": "^1.0.3",
        "@nestjs/core": "^8.1.2",
        "@nestjs/mongoose": "^9.0.1",
        "@nestjs/platform-express": "^8.1.2",
        "@nestjs/passport": "^8.0.1",
        "@nestjs/platform-socket.io": "^8.1.2",
        "@nestjs/serve-static": "^2.2.2",
        "@nestjs/swagger": "^5.1.4",
        "passport": "^0.5.0",
        "passport-firebase-jwt": "^1.2.1",
        "@nestjs/terminus": "^8.0.1",
        "@nestjs/typeorm": "^8.0.2",
        "@nestjs/websockets": "^8.1.2",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "express-session": "^1.17.2",
        "firebase": "^9.3.0",
        "firebase-admin": "^9.8.0",
        "firebase-functions": "^3.14.1",
        "firebase-tools": "^9.21.0",
        "mongoose": "^6.0.12",
        "mysql2": "^2.3.2",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.1.0",
        "swagger-ui-express": "^4.1.6",
        "typeorm": "^0.2.38"
    },
    "devDependencies": {
        "@compodoc/compodoc": "^1.1.15",
        "@nestjs/cli": "^7.6.0",
        "@nestjs/schematics": "^7.3.0",
        "@nestjs/testing": "^8.1.2",
        "@types/express": "^4.17.11",
        "@types/express-session": "^1.17.4",
        "@types/jest": "^26.0.22",
        "@types/node": "^14.14.36",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "eslint": "^7.22.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "firebase-functions-test": "^0.2.0",
        "jest": "^26.6.3",
        "jest-junit": "^13.0.0",
        "prettier": "^2.2.1",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.4",
        "ts-loader": "^8.0.18",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.2.3"
    }
}
