{
  "name": "cargonaut",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "predev": "npm run all:build",
    "dev": "concurrently \"npm run client:start\" \"npm run api:start\"",
    "client:start": "nx serve",
    "client:build": "nx build",
    "client:test": "nx test",
    "api:build": "nx build api",
    "api:start": "nx serve api",
    "api:test": "nx test",
    "all:lint": "nx run-many --target=lint --all --parallel",
    "all:e2e": "nx run-many --target=e2e --all --parallel",
    "all:test": "nx run-many --target=test --all --parallel",
    "all:build": "nx run-many --target=build --all --parallel",
    "all:doc": "nx run-many --target=compodoc --all --parallel",
    "affected:lint": "nx affected --target=lint --parallel",
    "affected:test": "nx affected --target=test --parallel",
    "affected:build": "nx affected --target=build --parallel",
    "affected:doc": "nx affected --target=compodoc --parallel",
    "affected:e2e": "nx affected --target=e2e --parallel",
    "ci:e2e": "nx affected --target=e2e --parallel --base=main~1 --head=main --verbose",
    "ci:test": "nx affected --target=test --parallel --base=main~1 --head=main --verbose",
    "ci:lint": "nx affected --target=lint --parallel --base=main~1 --head=main --verbose",
    "ci:doc": "nx affected --target=doc --parallel --base=main~1 --head=main --verbose",
    "ci:build": "nx affected --target=build --parallel --base=main~1 --head=main --verbose",
    "format": "nx format:write",
    "format:check": "nx format:check --files apps/**/*.ts libs/**/*.ts",
    "deps": "nx dep-graph"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.0.0",
    "@angular/common": "^13.0.0",
    "@angular/compiler": "^13.0.0",
    "@angular/core": "^13.0.0",
    "@angular/forms": "^13.0.0",
    "@angular/platform-browser": "^13.0.0",
    "@angular/platform-browser-dynamic": "^13.0.0",
    "@angular/router": "^13.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nrwl/angular": "13.2.3",
    "concurrently": "^6.4.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.0",
    "@angular-eslint/eslint-plugin": "~12.6.0",
    "@angular-eslint/eslint-plugin-template": "~12.6.0",
    "@angular-eslint/template-parser": "~12.6.0",
    "@angular/cli": "~13.0.0",
    "@angular/compiler-cli": "^13.0.0",
    "@angular/language-service": "^13.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.2.3",
    "@nrwl/cypress": "13.2.3",
    "@nrwl/eslint-plugin-nx": "13.2.3",
    "@nrwl/jest": "13.2.3",
    "@nrwl/linter": "13.2.3",
    "@nrwl/nest": "13.2.3",
    "@nrwl/node": "13.2.3",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "13.2.3",
    "@nrwl/workspace": "13.2.3",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "@typescript-eslint/parser": "~4.33.0",
    "cypress": "^8.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.2.3",
    "jest-preset-angular": "11.0.0",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.4.3"
  }
}
