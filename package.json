{
  "name": "cargonaut",
  "version": "0.0.1",
  "description": "A platform to share rides",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/clemenscodes/cargonaut"
  },
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "predev": "npm run all:build:dev",
    "preprod": "npm run all:build:prod",
    "serve:dev": "concurrently \"npm run client:start:dev\" \"npm run api:start:dev\"",
    "dev": "concurrently \"npm run serve:dev\" \"npm run serve\"",
    "serve": "firebase serve",
    "firebase": "firebase emulators:start --import=.firebase --export-on-exit",
    "firebase:serve:api": "firebase emulators:start --only functions --import=.firebase --export-on-exit",
    "firebase:serve:client": "firebase emulators:start --only hosting --import=.firebase --export-on-exit",
    "client:test": "nx test",
    "client:build:dev": "nx build --configuration=development --skip-nx-cache",
    "client:start:dev": "nx serve --configuration=development",
    "client:build:prod": "nx build --skip-nx-cache",
    "client:start:prod": "nx serve",
    "api:test": "nx test api",
    "api:build:dev": "nx build api --configuration=development --skip-nx-cache",
    "api:start:dev": "nx serve api --configuration=development",
    "api:build:prod": "nx build api --skip-nx-cache",
    "api:start:prod": "nx serve api",
    "all:build:dev": "nx run-many --target=build --all --parallel --configuration=development --skip-nx-cache",
    "all:build:prod": "nx run-many --target=build --all --parallel --skip-nx-cache",
    "all:lint": "nx run-many --target=lint --all --parallel --skip-nx-cache",
    "all:doc": "nx run-many --target=compodoc --all --parallel --skip-nx-cache",
    "all:build": "nx run-many --target=build --all --parallel --skip-nx-cache",
    "all:test": "nx run-many --target=test --all --parallel --skip-nx-cache",
    "all:e2e": "nx run-many --target=e2e --all --parallel --skip-nx-cache",
    "ci:lint": "nx affected --target=lint --parallel --max-parallel=3",
    "ci:test": "nx affected --target=test --parallel  --max-parallel=3",
    "ci:build": "nx affected --target=build --parallel  --max-parallel=3 --skip-nx-cache",
    "ci:doc": "nx affected --target=compodoc --parallel  --max-parallel=3",
    "ci:e2e": "nx affected --target=e2e --parallel  --max-parallel=3",
    "format": "nx format:write",
    "format:check": "nx format:check --files apps/**/*.ts libs/**/*.ts",
    "deps": "nx dep-graph",
    "help": "nx help"
  },
  "dependencies": {
    "@angular/animations": "^13.1.0",
    "@angular/common": "^13.1.0",
    "@angular/compiler": "^13.1.0",
    "@angular/core": "^13.1.0",
    "@angular/fire": "^7.2.0",
    "@angular/forms": "^13.1.0",
    "@angular/localize": "^13.1.0",
    "@angular/platform-browser": "^13.1.0",
    "@angular/platform-browser-dynamic": "^13.1.0",
    "@angular/pwa": "^13.1.0",
    "@angular/router": "^13.1.0",
    "@angular/service-worker": "^13.1.0",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "~1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.1.5",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@nrwl/angular": "13.2.3",
    "bootstrap": "^4.5.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "firebase": "^9.6.1",
    "firebase-admin": "^10.0.0",
    "firebase-functions": "^3.16.0",
    "reflect-metadata": "^0.1.13",
    "rxfire": "^6.0.3",
    "rxjs": "~7.4.0",
    "swagger-ui-express": "^4.2.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.0",
    "@angular-eslint/eslint-plugin": "~12.6.0",
    "@angular-eslint/eslint-plugin-template": "~12.6.0",
    "@angular-eslint/template-parser": "~12.6.0",
    "@angular/cli": "~13.0.0",
    "@angular/compiler-cli": "^13.1.0",
    "@angular/language-service": "^13.1.0",
    "@compodoc/compodoc": "^1.1.15",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.2.3",
    "@nrwl/cypress": "13.2.3",
    "@nrwl/eslint-plugin-nx": "13.2.3",
    "@nrwl/jest": "13.2.3",
    "@nrwl/linter": "13.2.3",
    "@nrwl/nest": "13.2.3",
    "@nrwl/node": "13.2.3",
    "@nrwl/nx-cloud": "12.5.4",
    "@nrwl/tao": "13.2.3",
    "@nrwl/workspace": "13.2.3",
    "@twittwer/compodoc": "^1.6.5",
    "@types/jest": "27.0.2",
    "@types/node": "^14.18.0",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "@typescript-eslint/parser": "~4.33.0",
    "concurrently": "^6.4.0",
    "cypress": "^8.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.2.3",
    "jest-preset-angular": "11.0.0",
    "prettier": "^2.5.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.4.3"
  }
}
