{
    "name": "cargonaut",
    "version": "0.0.1",
    "description": "A platform to share rides",
    "license": "MIT",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/clemenscodes/cargonaut"
    },
    "engines": {
        "node": "14"
    },
    "main": "dist/apps/api/main.js",
    "scripts": {
        "ng": "nx",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
        "predev": "npm run all:build:dev",
        "preprod": "npm run all:build:prod",
        "dev": "concurrently \"npm run client:start:dev\" \"npm run api:start:dev\"",
        "prod": "concurrently \"npm run client:start:prod\" \"npm run api:start:prod\"",
        "prefirebase": "npm run all:build:dev",
        "firebase": "firebase emulators:start --import=.firebase --export-on-exit",
        "firebase:serve:api": "firebase emulators:start --only functions --import=.firebase --export-on-exit",
        "firebase:serve:client": "firebase emulators:start --only hosting --import=.firebase --export-on-exit",
        "client:test": "nx test",
        "client:build:dev": "nx build --configuration=development",
        "client:start:dev": "nx serve --configuration=development",
        "client:build:prod": "nx build",
        "client:start:prod": "nx serve",
        "api:test": "nx test api",
        "api:build:dev": "nx build api --configuration=development",
        "api:start:dev": "nx serve api --configuration=development",
        "api:build:prod": "nx build api",
        "api:start:prod": "nx serve api",
        "all:build:dev": "nx run-many --target=build --all --parallel --configuration=development",
        "all:build:prod": "nx run-many --target=build --all --parallel",
        "all:lint": "nx run-many --target=lint --all --parallel",
        "all:doc": "nx run-many --target=compodoc --all --parallel",
        "all:build": "nx run-many --target=build --all --parallel",
        "all:test": "nx run-many --target=test --all --parallel",
        "all:e2e": "nx run-many --target=e2e --all --parallel",
        "ci:lint": "nx run-many --target=lint --all --parallel --skip-nx-cache",
        "ci:doc": "nx run-many --target=compodoc --all --parallel --skip-nx-cache",
        "ci:build": "nx run-many --target=build --all --parallel --skip-nx-cache",
        "ci:test": "nx run-many --target=test --all --parallel --skip-nx-cache",
        "ci:e2e": "nx run-many --target=e2e --all --parallel --skip-nx-cache",
        "affected:lint": "nx affected --target=lint --parallel",
        "affected:test": "nx affected --target=test --parallel",
        "affected:build": "nx affected --target=build --parallel",
        "affected:doc": "nx affected --target=compodoc --parallel",
        "affected:e2e": "nx affected --target=e2e --parallel",
        "format": "nx format:write",
        "format:check": "nx format:check --files apps/**/*.ts libs/**/*.ts",
        "deps": "nx dep-graph",
        "help": "nx help"
    },
    "dependencies": {
        "@angular/animations": "^13.0.0",
        "@angular/common": "^13.0.0",
        "@angular/compiler": "^13.0.0",
        "@angular/core": "^13.0.0",
        "@angular/fire": "^7.2.0",
        "@angular/forms": "^13.0.0",
        "@angular/platform-browser": "^13.0.0",
        "@angular/platform-browser-dynamic": "^13.0.0",
        "@angular/router": "^13.0.0",
        "@nestjs/common": "^8.0.0",
        "@nestjs/core": "^8.0.0",
        "@nestjs/platform-express": "^8.0.0",
        "@nestjs/swagger": "^5.1.5",
        "@nrwl/angular": "13.2.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "firebase-admin": "^10.0.0",
        "firebase-functions": "^3.16.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "~7.4.0",
        "swagger-ui-express": "^4.2.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.0.0",
        "@angular-eslint/eslint-plugin": "~12.6.0",
        "@angular-eslint/eslint-plugin-template": "~12.6.0",
        "@angular-eslint/template-parser": "~12.6.0",
        "@angular/cli": "~13.0.0",
        "@angular/compiler-cli": "^13.0.0",
        "@angular/language-service": "^13.0.0",
        "@compodoc/compodoc": "^1.1.15",
        "@nestjs/schematics": "^8.0.0",
        "@nestjs/testing": "^8.0.0",
        "@nrwl/cli": "13.2.3",
        "@nrwl/cypress": "13.2.3",
        "@nrwl/eslint-plugin-nx": "13.2.3",
        "@nrwl/jest": "13.2.3",
        "@nrwl/linter": "13.2.3",
        "@nrwl/nest": "13.2.3",
        "@nrwl/node": "13.2.3",
        "@nrwl/nx-cloud": "latest",
        "@nrwl/tao": "13.2.3",
        "@nrwl/workspace": "13.2.3",
        "@twittwer/compodoc": "^1.6.5",
        "@types/jest": "27.0.2",
        "@types/node": "14.14.33",
        "@typescript-eslint/eslint-plugin": "~4.33.0",
        "@typescript-eslint/parser": "~4.33.0",
        "concurrently": "^6.4.0",
        "cypress": "^8.3.0",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-cypress": "^2.10.3",
        "jest": "27.2.3",
        "jest-preset-angular": "11.0.0",
        "prettier": "^2.3.1",
        "ts-jest": "27.0.5",
        "typescript": "~4.4.3"
    }
}
